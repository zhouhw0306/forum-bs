<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.SourceMapper">

    <resultMap id="BaseResultMap" type="com.example.domain.Source">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="title" column="title" jdbcType="VARCHAR"/>
            <result property="description" column="description" jdbcType="VARCHAR"/>
            <result property="category" column="category" jdbcType="VARCHAR"/>
            <result property="content" column="content" jdbcType="VARCHAR"/>
            <result property="cover" column="cover" jdbcType="VARCHAR"/>
            <result property="thumbNum" column="thumb_num" jdbcType="INTEGER"/>
            <result property="favourNum" column="favour_num" jdbcType="INTEGER"/>
            <result property="state" column="state" jdbcType="INTEGER"/>
            <result property="isDelete" column="is_delete" jdbcType="CHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="userId" column="user_id" jdbcType="CHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,title,description,
        category,content,cover,
        thumbNum,favourNum,priority,
        isDelete,createTime,userId
    </sql>
</mapper>
